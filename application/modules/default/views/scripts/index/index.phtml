<div id="loader"></div>
<div class="imageresizer">
	<div class="imageresizer__block">
		<div class="dnd"><img src="media/images/og_grrr.png"><p>Drag and Drop the Files</p></div>
	</div>
		<div class="imageresizer__input">
			<input type="file" id="file-input" multiple />
			<input type="number" id="widthVal" placeholder="width" />
			<input type="number" id="heightVal" placeholder="height" />
		</div>
		<div id="imageresizer__actions">
			<button type="button" id="resize">resize</button>
			<button type="button" id="crop">crop</button>
			<button type="button" id="clear">clear</button>
			<button type="button" id="antialias">anti-alias</button>
			<button id="download">download</button>
		</div>
		<img class="logo" src="media/images/lama.png">
</div>
<div id="imagecollection">
	<div id="imagecollection__list"></div>
</div>
<form class="outputfiles">
	<div id="outputfiles__storage">
<!-- 		<br/><p>Original was 1600x1200, reduced to 400x300 canvas</p><br/>
		<canvas id="canvas" width=100 height=100></canvas> -->
		<div id="items">
		</div>
	</div>
</form>
